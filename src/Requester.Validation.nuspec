<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>Requester.Validation</id>
        <title>Requester - for Web API validation</title>
        <version>$version$</version>
        <authors>danielwertheim</authors>
        <copyright>Â©Daniel Wertheim</copyright>
        <summary>For validation of HTTP web API JSON responses.</summary>
        <description>Assists with validating responses from web APIs. E.g. by validation of JSON-schema etc.</description>
        <projectUrl>https://github.com/danielwertheim/requester</projectUrl>
        <licenseUrl>https://raw.githubusercontent.com/danielwertheim/requester/master/LICENSE.txt</licenseUrl>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <tags>http web api json response validation</tags>
        <frameworkAssemblies>
          <frameworkAssembly assemblyName="System.Net.Http" targetFramework="net45" />
        </frameworkAssemblies>
        <dependencies>
           <group targetFramework="net45">
             <dependency id="Requester" version="[$version$]" />
             <dependency id="Ensure.That" version="5.0.0" />
             <dependency id="Newtonsoft.Json" version="9.0.1" />
           </group>
        </dependencies>
    </metadata>
    <files>
        <file src="projects\Requester.Validation\bin\$configuration$\Requester.Validation.dll" target="lib\net45"/>
        <file src="projects\Requester.Validation\bin\$configuration$\Requester.Validation.xml" target="lib\net45"/>
    </files>
</package>